
<% layout("/layouts/boilerplate.ejs") %>
<body>
  <div class="row">
    <%for(let listing of allStrings){  %>
      <a class="link" href="/listings/<%= listing._id %>">
      <div class="card" style="width: 18rem;">
        <img style="height: 200px; border-radius:10px ;" src="<%= listing.image.url %>" class="card-img-top" alt="...">
        <div class="card-img-overlay"></div>
        <div class="card-body">
          <p class="card-text">&#8377;<%= listing.price.toLocaleString("en-IN") %></p>
          <p><b><%= listing.title %></b></p>
        </div>
      </div>
    </a>
      <%} %>
  </div>
 
</body>



